<!DOCTYPE html>
<html>
    {%include "default/head.html.twig"%}
    <body>
		<div id="wrapper">
			<div class="container header">
				<div class="h-left">
					<div class="logo"></div>
					<p class="slogan">
						SLSMPRO
					</p>
				</div>
				<div class="h-right">
					{% block hright%}{% endblock %}
				</div>
			</div>
			<div class="container-fluid menu">
				{% block nemu %}
				{% endblock %}
			</div>
			<div id="line"/>
			<div class="container-fluid text-center">    
				<div class="row content">
						<div class="container sub-menu">
							<ul class="list-inline">
								{% block smenu %}{% endblock %}			
							</ul>
						</div>
					<div class="col-sm-8 text-left">
						<div class="container-content">
							<div class="message-content">
								{%include "default/message.html.twig"%}
							</div>
							<legend>{% block label %}{% endblock %}</legend>
							{% block actions %}{% endblock %}
							{% block body %}{% endblock %}
						</div>
					</div>
					<div class="col-sm-2 sidenav">
					  <div class="well">
							{% block nav %}{% endblock %}		
					  </div>
					 {%if dnb is defined%}
					  <div class="fine">
							<div>{{'label.actions'|trans([],'BoAdminBundle')}}</div>
							{% block navbis %}{% endblock %}		
					  </div>
					 {%endif%}
					</div>
				</div>
			</div>
			{%include "default/footer.html.twig"%}
		</div>
        {% block javascripts %}
		<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
		<script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>			
	{% endblock %}
    </body>
</html>
